{
  "name": "mgdg",
  "version": "1.0.3",
  "description": "Mock Graphql Data Generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/donovanhiland/mgdg.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch-build": "npx tsc-watch",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx,js,snap",
    "format": "prettier 'src/**/*.{ts,tsx}'",
    "format:write": "yarn format --list-different --write",
    "format:check": "yarn format --check",
    "static": "npm-run-all --parallel -c tsc lint format:check",
    "test": "jest",
    "validate": "npm-run-all static test"
  },
  "keywords": [],
  "author": "Donovan Hiland",
  "license": "ISC",
  "dependencies": {
    "apollo-utilities": "^1.3.4",
    "graphql-tools": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "babel-jest": "^26.6.3",
    "eslint": "^7.15.0",
    "eslint-config-kentcdodds": "^17.3.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0"
  }
}
